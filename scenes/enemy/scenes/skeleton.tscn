script/source = "extends PlatformerEnemy

@export var reach = 25
@onready var animation_tree = $Sprite/AnimationTree
@onready var sprite = $Sprite
@onready var players = get_tree().get_nodes_in_group(\"player\")
@onready var ground_left = $WalkLeft
@onready var ground_right = $WalkRight
@onready var stop_left = $WallLeft
@onready var stop_right = $WallRight
@onready var weapon: GenericWeapon = $Weapon.get_children()[0]
@onready var input_map: ControlledInputMap = $InputMap
var patrol_direction = randi_range(0, 1) * 2 - 1
var patrol_time = 0

func _ready():
	super()
	weapon._set_data(input_map, team)
	weapon.parent = self

func animate():
	var set_p = func (property, value):
		animation_tree[\"parameters/\" + property] = value
	var direction = trigger.direction
	
	if direction != 0: sprite.flip_h = direction < 0
	set_p.call(\"is_walking/transition_request\", \"walk\" if direction != 0 else \"static\")
	set_p.call(\"tilting/transition_request\", [\"left\", \"static\", \"right\"][round(direction) + 1])
	set_p.call(\"in_air/transition_request\", \"ground\" if is_on_floor() else \"air\")

func _process(delta):
	var target_distance = INF
	
	if target:
		target_distance = position.distance_to(target.position)
		if target_distance <= reach:
			input_map.press(\"attack\")
		else:
			input_map.release(\"attack\")
		
		if path.size() > 0:
			var distance = path[0] - position
			trigger.direction = sign(distance.x)
			trigger.jump = distance.y < 0
			
			if has_los_to(target.position): 
				target_position = target.position
			
			pathfind_to(target_position)
			path.pop_front()
		elif has_los_to(target.position):
			target_position = target.position
			pathfind_to(target.position)
			path.pop_front()
		else:
			target = null
	else:
		for player in players:
			if has_los_to(player.position):
				target = player
				pathfind_to(player.position)
		
		if patrol_time == 0:
			match patrol_direction:
				1:
					if ground_right.is_colliding() and not stop_right.is_colliding():
						trigger.direction = 1
					else:
						patrol_direction = -1
						patrol_time = randf_range(2, 5)
				-1:
					if ground_left.is_colliding() and not stop_left.is_colliding():
						trigger.direction = -1
					else:
						patrol_direction = 1
						patrol_time = randf_range(2, 5)
		else: 
			patrol_time = move_toward(patrol_time, 0, delta)
			trigger.direction = 0
	
	animate()
	
	weapon._process_weapon(
			Vector2(trigger.direction, 0) 
			if target_distance > reach * 4 or target == null
			else (target.position - position).normalized())

"

